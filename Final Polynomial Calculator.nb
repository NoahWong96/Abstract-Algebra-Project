Cell[CellGroupData[{

Cell["\<\
This section calculates the order of a specific element g. Just change the \
first line of the code below. Replace the polynomial after \
\[OpenCurlyDoubleQuote]g\[CloseCurlyDoubleQuote] with the polynomial you wish \
to test making sure to end the line with a semicolon. Then on your keyboard \
press \[OpenCurlyDoubleQuote]Shift\[CloseCurlyDoubleQuote] and \
\[OpenCurlyDoubleQuote]Enter\[CloseCurlyDoubleQuote] to run the code. The \
results should return below in blue.\
\>", "Chapter",
 CellChangeTimes->{{3.785694543277026*^9, 3.785694654543174*^9}, {
  3.7856947770648923`*^9, 3.785694802777798*^9}, {3.7856948343279896`*^9, 
  3.785694834562585*^9}, {3.7856949253331757`*^9, 
  3.7856949271765127`*^9}},ExpressionUUID->"226ae9a7-aca1-4486-9635-\
6bd577a86ffe"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  StyleBox[
   RowBox[{
    RowBox[{"g", " ", "=", " ", 
     RowBox[{
      RowBox[{"x", "^", "2"}], "+", "3"}]}], ";"}],
   FontSize->24], "\[IndentingNewLine]", 
  StyleBox[
   RowBox[{"(*", "  ", 
    RowBox[{
     RowBox[{"Only", " ", "change", " ", "g"}], ",", " ", 
     RowBox[{
     "do", " ", "not", " ", "change", " ", "anything", " ", "below", " ", 
      "this"}]}], " ", "*)"}],
   FontSize->14]}], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{"gOrder", " ", "=", " ", 
    RowBox[{"OrderVector", "[", 
     RowBox[{"[", 
      RowBox[{"PolyToN", "[", 
       RowBox[{
        RowBox[{"PolynomialRemainder", "[", 
         RowBox[{"g", ",", "p1", ",", "x", ",", 
          RowBox[{"Modulus", "\[Rule]", " ", "p"}]}], "]"}], ",", "p"}], 
       "]"}], "]"}], "]"}]}], ";"}],
  FontSize->14], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{"gOrder", " ", "\[Equal]", " ", 
      RowBox[{"-", "1"}]}], ",", 
     RowBox[{"Print", "[", 
      RowBox[{"Style", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"ToString", "[", 
          RowBox[{"g", ",", "InputForm"}], "]"}], "<>", " ", 
         "\"\< is a zero divisor\>\""}], ",", "Bold", ",", " ", "Color1", ",",
         " ", 
        RowBox[{"FontSize", "\[Rule]", "SizeFont"}]}], "]"}], "]"}], ",", 
     RowBox[{"Print", "[", 
      RowBox[{"Style", "[", 
       RowBox[{
        RowBox[{"\"\<The order of \>\"", " ", "<>", " ", 
         RowBox[{"ToString", "[", 
          RowBox[{"g", ",", "InputForm"}], "]"}], "<>", " ", "\"\< is \>\"", "<>",
          " ", 
         RowBox[{"ToString", "[", "gOrder", "]"}]}], ",", "Bold", ",", " ", 
        "Color1", ",", " ", 
        RowBox[{
         StyleBox["FontSize",
          FontSize->14], 
         StyleBox["\[Rule]",
          FontSize->14], " ", "SizeFont"}]}], "]"}], "]"}]}], "]"}], ";"}],
  FontSize->14]}], "Input",
