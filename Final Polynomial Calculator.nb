Cell[BoxData[
 StyleBox[
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", " ", "=", "2"}], ",", 
    RowBox[{"i", " ", "<", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Length", "[", "OrderVector", "]"}], "+", "1"}], ")"}]}], ",", 
    " ", 
    RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"OrderVector", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "\[Equal]", "0"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"bool", " ", "=", " ", "True"}], ";", "\[IndentingNewLine]", 
       RowBox[{"t", "=", 
        RowBox[{"{", "i", "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"f", "=", " ", 
        RowBox[{"nToPoly", "[", 
         RowBox[{"i", ",", "p"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"g", " ", "=", "f"}], ";", "\[IndentingNewLine]", 
       RowBox[{"n", "=", "1"}], ";", "\[IndentingNewLine]", 
       RowBox[{"While", "[", 
        RowBox[{"bool", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"n", "++"}], ";", "\[IndentingNewLine]", 
          RowBox[{"g", " ", "=", " ", 
           RowBox[{"PolynomialRemainder", "[", 
            RowBox[{
             RowBox[{"g", " ", "*", " ", "f"}], ",", "p1", ",", "x", ",", 
             RowBox[{"Modulus", " ", "\[Rule]", "p"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"CoefficientList", "[", 
              RowBox[{
               RowBox[{"g", "-", "1"}], " ", ",", "x"}], "]"}], "\[Equal]", 
             RowBox[{"{", "}"}]}], ",", " ", 
            RowBox[{
             RowBox[{"bool", " ", "=", " ", "False"}], ";", " ", 
             RowBox[{
              RowBox[{"OrderVector", "[", 
               RowBox[{"[", "i", "]"}], "]"}], "=", "n"}], ";"}], ",", " ", 
            RowBox[{
             RowBox[{"t", " ", "=", " ", 
              RowBox[{"Join", "[", 
               RowBox[{"t", ",", 
                RowBox[{"{", 
                 RowBox[{"PolyToN", "[", 
                  RowBox[{"g", ",", "p"}], "]"}], "}"}]}], "]"}]}], ";"}]}], 
           "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
