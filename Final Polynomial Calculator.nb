          RowBox[{
           RowBox[{"OrderVector", "[", 
            RowBox[{"[", 
             RowBox[{"t", "[", 
              RowBox[{"[", "j", "]"}], "]"}], "]"}], "]"}], "=", 
           RowBox[{"n", "/", 
            RowBox[{"GCD", "[", 
             RowBox[{"n", ",", "j"}], "]"}]}]}], ";"}]}], "]"}]}]}], 
