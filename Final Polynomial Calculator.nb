Cell[CellGroupData[{

Cell["\<\
This section returns the polynomials of a specific order. Just change the \
first line of code below. Replace the number after \
\[OpenCurlyDoubleQuote]FindOrder\[CloseCurlyDoubleQuote] with the order you \
wish to test making sure to end the line with a semicolon. Then on your \
keyboard press \[OpenCurlyDoubleQuote]Shift\[CloseCurlyDoubleQuote] and \
\[OpenCurlyDoubleQuote]Enter\[CloseCurlyDoubleQuote] to run the code. The \
results should return below in blue. It will return just the first 50 \
polynomials. You can also change \[OpenCurlyDoubleQuote]numberpolynomails\
\[CloseCurlyDoubleQuote] if you wish to return more results.\
\>", "Chapter",
 CellChangeTimes->{{3.7856947295373306`*^9, 3.7856947692857285`*^9}, {
  3.785694808854336*^9, 
  3.7856949203653708`*^9}},ExpressionUUID->"13a8156f-2a9c-4868-969c-\
4bc933a8641f"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FindOrder", " ", "=", " ", "10"}], ";"}], "\[IndentingNewLine]", 
  StyleBox[
   RowBox[{"(*", "  ", 
    RowBox[{
     RowBox[{"Only", " ", "change", " ", "FindOrder"}], ",", " ", 
     RowBox[{
     "do", " ", "not", " ", "change", " ", "anything", " ", "below", " ", 
      "this"}]}], " ", "*)"}],
   FontSize->14]}], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{"numberpolynomials", " ", "=", " ", "50"}], ";"}],
  FontSize->14], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{"OrdertoPrint", " ", "=", " ", 
    RowBox[{"{", "}"}]}], ";"}],
  FontSize->14], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"i", "=", "0"}], ",", 
     RowBox[{"i", "<", "N0"}], ",", 
     RowBox[{"i", "++"}], ",", " ", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"OrderVector", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "\[Equal]", "FindOrder"}], ",", 
       RowBox[{"OrdertoPrint", "=", 
        RowBox[{"Join", "[", 
         RowBox[{"OrdertoPrint", ",", 
          RowBox[{"{", " ", 
           RowBox[{"nToPoly", "[", 
            RowBox[{"i", ",", "p"}], "]"}], "}"}]}], "]"}]}]}], "]"}]}], 
    "]"}], ";"}],
  FontSize->14], "\[IndentingNewLine]", 
 StyleBox[
  RowBox[{
   RowBox[{"Print", "[", 
    RowBox[{"Style", "[", 
     RowBox[{
      RowBox[{"OrdertoPrint", "[", 
       RowBox[{"[", 
        RowBox[{"1", ";;", 
         RowBox[{"Min", "[", 
          RowBox[{"numberpolynomials", ",", " ", 
           RowBox[{"Length", "[", "OrdertoPrint", "]"}]}], "]"}]}], "]"}], 
       "]"}], ",", "Bold", ",", " ", "Color1", ",", " ", 
      RowBox[{"FontSize", "\[Rule]", "SizeFont"}]}], "]"}], "]"}], ";"}],
  FontSize->14]}], "Input",
